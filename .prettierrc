{
	"trailingComma": "none",
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^@/prisma/(.*)$",
		"^@/components/ui(.*)$",
		"^@/components/layouts(.*)$",
		"^@/pages/(.*)$",
		"^@/store/(.*)$",
		"^@/hooks/(.*)$",
		"^@/utils/(.*)$",
		"^@/provider/(.*)$",
		"^@/@types/(.*)$",
		"^@/.*$",
		"^../(.*)$",
		"^./(?!.*\\.module\\.scss$).*$",
		"^./.*\\.module\\.scss$",
		"(.scss)$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
